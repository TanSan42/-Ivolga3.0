import telebot
from telebot import types
import requests


# Замените 'YOUR_TELEGRAM_BOT_TOKEN' на токен вашего бота
TOKEN = '7412317351:AAE9efWRzzt4QJqCxIIQMAppExBvIgvqHhs'
bot = telebot.TeleBot(TOKEN)

# Функция для отправки изображения из URL
def send_image_from_url(chat_id, image_url):
    try:
        response = requests.get(image_url)
        if response.status_code == 200:
            bot.send_photo(chat_id, response.content)
        else:
            bot.send_message(chat_id, "Не удалось получить изображение.")
    except Exception as e:
        bot.send_message(chat_id, f"Возникла ошибка: {str(e)}")



# Обработчик команды /start
@bot.message_handler(commands=['start'])
def start(message):
    # Создаем клавиатуру с кнопкой "Запуск бота"
    keyboard = types.ReplyKeyboardMarkup(resize_keyboard=True)
    button_start = types.KeyboardButton("Запуск бота")
    keyboard.add(button_start)

    bot.send_message(message.chat.id, "Добро пожаловать! Нажмите на кнопку ниже для запуска:", reply_markup=keyboard)


# Обработчик всех текстовых сообщений
@bot.message_handler(func=lambda message: True)
def echo_all(message):
    if message.text == "Запуск бота":
        # Создаем клавиатуру с кнопкой "Техкарты"
        keyboard = types.ReplyKeyboardMarkup(resize_keyboard=True)
        button = types.KeyboardButton("Техкарты")
        keyboard.add(button)

        bot.send_message(message.chat.id, "Бот запущен! Нажмите на кнопку ниже:", reply_markup=keyboard)

    elif message.text == "Техкарты":
        bot.send_message(message.chat.id, "Вы нажали кнопку!")

